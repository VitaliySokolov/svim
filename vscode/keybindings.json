[
  {
    "key": "ctrl+q ctrl+q",
    "command": "workbench.action.quit"
  },
  {
    "key": "ctrl+shift+enter",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.terminal.focus",
    "when": "accessibilityModeEnabled && accessibleViewOnLastLine && terminalHasBeenCreated && accessibleViewCurrentProviderId == 'terminal' || accessibilityModeEnabled && accessibleViewOnLastLine && terminalProcessSupported && accessibleViewCurrentProviderId == 'terminal'"
  },
  {
    "key": "cmd+down",
    "command": "-workbench.action.terminal.focus",
    "when": "accessibilityModeEnabled && accessibleViewOnLastLine && terminalHasBeenCreated && accessibleViewCurrentProviderId == 'terminal' || accessibilityModeEnabled && accessibleViewOnLastLine && terminalProcessSupported && accessibleViewCurrentProviderId == 'terminal'"
  },
  {
    "key": "ctrl+\\",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.focusLeftGroup",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.focusBelowGroup",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.focusAboveGroup",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.focusRightGroup",
    "when": "editorFocus"
  },
  {
    "key": "cmd+x",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "alt+x",
    "command": "-workbench.action.showCommands"
  },
  // {
  //   "key": "escape",
  //   "command": "workbench.action.toggleSidebarVisibility",
  //   "when": "sideBarFocus"
  // },
  // 1. If the widget is visible but DOES NOT have multiple suggestions, Tab accepts.
  // {
  //   "key": "tab",
  //   "command": "acceptSelectedSuggestion",
  //   "when": "suggestWidgetVisible && !suggestWidgetMultipleSuggestions && textInputFocus"
  // },
  // // 2. If the widget is visible and DOES have multiple suggestions, Tab cycles.
  // {
  //   "key": "tab",
  //   "command": "selectNextSuggestion",
  //   "when": "suggestWidgetVisible && suggestWidgetMultipleSuggestions && textInputFocus"
  // },
  {
    "key": "shift+tab",
    "command": "hideSuggestWidget",
    "when": "suggestWidgetVisible"
  },
  // {
  //   "key": "tab",
  //   "command": "-acceptSelectedSuggestion",
  //   "when": "suggestWidgetHasFocusedSuggestion && suggestWidgetVisible && textInputFocus"
  // },
  // PREVENT TAB FROM JUMPING OUT:
  // Captures Tab in the Debug Console and does nothing if the widget is hidden.
  {
    "key": "tab",
    "command": "noop",
    "when": "inDebugRepl && !suggestWidgetVisible"
  },
  {
    "key": "shift+tab",
    "command": "noop",
    "when": "inDebugRepl && !suggestWidgetVisible"
  },
  // // DEBUG CONSOLE: If only one suggestion exists, Tab accepts it
  // {
  //   "key": "tab",
  //   "command": "acceptSelectedSuggestion",
  //   "when": "inDebugRepl && suggestWidgetVisible && !suggestWidgetMultipleSuggestions"
  // },
  // DEBUG CONSOLE: If multiple suggestions exist, Tab cycles next
  // {
  //   "key": "tab",
  //   "command": "selectNextSuggestion",
  //   "when": "inDebugRepl && suggestWidgetVisible && suggestWidgetMultipleSuggestions"
  // },
  {
    "key": "shift+tab",
    "command": "hideSuggestWidget",
    "when": "inDebugRepl && suggestWidgetVisible"
  },
  // Unbind the default Tab behavior in the Debug Console to prevent conflicts
  // {
  //   "key": "tab",
  //   "command": "-acceptSelectedSuggestion",
  //   "when": "inDebugRepl && suggestWidgetVisible"
  // },
  // Debug Console: Enter to accept suggestion
  {
    "key": "enter",
    "command": "acceptSelectedSuggestion",
    "when": "inDebugRepl && suggestWidgetVisible"
  },
  {
    "key": "ctrl+b c",
    "command": "workbench.action.terminal.new",
    "when": "terminalProcessSupported || terminalWebExtensionContributedProfile"
  },
  {
    "key": "ctrl+b p",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "ctrl+b n",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "ctrl+b shift+\\",
    "command": "workbench.action.terminal.split",
    "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
  },
]
